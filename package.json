    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "highcharts": "^10.1.0",
    "highcharts-react-official": "^3.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
